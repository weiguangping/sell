html
  width 100%
  height 100%
  body
    height 100%
    background #c0c0c0
    font-size .20rem
    color #333
    .padding
      padding-bottom 1.1rem
    .pad_top
      padding-top 0.8rem
    .pad_bottom
      padding-bottom 1.1rem
    .p_s
      padding .15rem 0
    .p_h
      padding 0 .15rem
    .p_all
      padding .15rem
    .bg_white
      background white
    h1,h2,h3,h4,h5,h6
      font-weight normal
    a
      color #333
      display inline-block
      outline none
    input,button
      outline none
    .mt_10
      margin-top .1rem
    .mt_20
      margin-top .2rem
    .mb_10
      margin-bottom .1rem
    .mb_20
      margin-bottom .2rem
    .b_s
      border-top .01rem solid #f2f2f2
      border-bottom .01rem solid #f2f2f2
    .b_h
      border-left .01rem solid #f2f2f2
      border-right .01rem solid #f2f2f2
    .b_t
      border-top .01rem solid #f2f2f2
    .b_b
      border-bottom .01rem solid #f2f2f2
    .b_l
      border-left .01rem solid #f2f2f2
    .b_r
      border-right .01rem solid #f2f2f2
    .fl
      float left
    .fr
      float right
    .db
      display block
    .dib
      display inline-block
    .flex
      display flex
    .flex_cencer
      display flex
      align-items center
      justify-content center
    .fix
      position fixed
    .f_16
      font-size .16rem
    .p_footer
      padding-bottom 1.1rem      
    .box
      box-sizing border-box
    .footer 
        .fix
          >div
            display block
            -webkit-box-flex 1
            -ms-flex 1
            flex 1
            text-decoration none        
          .router-link-active
            a
              color red 
    .text 
      text-overflow ellipsis
      white-space nowrap
      overflow hidden
      display block
    .texts
      display -webkit-box
      -webkit-box-orient vertical
      -webkit-line-clamp 3
      overflow hidden
    .price
      color red
    .clear 
      clear both
    .ov
      clear both
      overflow hidden 